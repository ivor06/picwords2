{
  "name": "picwords2-client",
  "version": "0.0.1",
  "description": "Client-side for picwords2",
  "scripts": {
    "postinstall": "typings install && gulp build-frontend",
    "start": "node build-server/app.js/",
    "build": "node_modules/.bin/webpack -p",
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "clean:aot": "rimraf -- compiled",
    "clean:dist": "rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "watch:dev": "npm run build:dev -- --watch",
    "watch": "npm run watch:dev"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.10",
    "awesome-typescript-loader": "^3.1.2",
    "browser-resolve": "^1.11.2",
    "compression": "^1.6.2",
    "del": "^2.2.2",
    "event-stream": "^3.3.4",
    "file-exists": "^3.0.2",
    "glob-intersection": "^0.1.3",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-file": "^0.3.0",
    "gulp-if": "^2.0.2",
    "gulp-inject": "^4.2.0",
    "gulp-inject-string": "^1.1.0",
    "gulp-ngc": "^0.2.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rollup": "^2.11.0",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-typescript": "^3.0.1",
    "gulp-uglifyjs": "^0.6.2",
    "gulp-util": "^3.0.0",
    "md5": "^2.2.1",
    "node.extend": "^1.1.6",
    "openurl": "^1.1.1",
    "path": "^0.12.7",
    "rollup-plugin-alias": "^1.2.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript2": "^0.4.0",
    "rollup-stream": "^1.19.0",
    "run-sequence": "^1.2.2",
    "streamqueue": "^1.1.1",
    "tslib": "^1.6.0",
    "typescript": ">=2.1.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-paths": "^2.1.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^2.3.3",
    "webpack-merge": "^4.1.0",
    "assets-webpack-plugin": "^3.5.1",
    "copy-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^2.28.0"
  },
  "dependencies": {
    "@angular/common": "^2.2.3",
    "@angular/compiler": "^2.2.3",
    "@angular/core": "^2.2.3",
    "@angular/forms": "^2.2.3",
    "@angular/http": "^2.2.3",
    "@angular/platform-browser": "^2.2.3",
    "@angular/platform-browser-dynamic": "^2.2.3",
    "@angular/router": "^3.2.3",
    "@angular/upgrade": "^2.2.3",
    "bootstrap": "^4.1.2",
    "core-js": ">=2.4.1",
    "jsonschema": "^1.1.0",
    "primeng": "^1.0.0",
    "querystring": "^0.2.0",
    "reflect-metadata": ">=0.1.3",
    "rxjs": "^5.0.0-rc.4",
    "systemjs": ">=0.19.27",
    "url": "^0.11.0",
    "zone.js": ">=0.7.2"
  }
}
